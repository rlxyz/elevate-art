{
  "name": "@elevateart/ui",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist",
    "./globals.css": "./dist/globals.css"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts --external react && tailwindcss -i ./src/styles/globals.css -o ./dist/globals.css",
    "dev": "concurrently \"tsup src/index.tsx --format esm,cjs --dts --external react --watch\" \"tailwindcss -i ./src/styles/globals.css -o ./dist/globals.css --watch\" \"yarn dev:storybook\"",
    "clean": "rm -rf dist .turbo node_modules storybook-static",
    "dev:storybook": "start-storybook -p 6006 -s public",
    "build:storybook": "concurrently \"yarn build\" \"build-storybook -s public\"",
    "serve-storybook": "serve storybook-static"
  },
  "files": [
    "src"
  ],
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "autoprefixer": "^10.4.12",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.2.2",
    "eslint": "^7.32.0",
    "postcss": "^8.4.18",
    "postcss-loader": "^7.0.1",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "tailwind-config": "*",
    "tailwindcss": "^3.1.8",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "tsup": "^6.1.3",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.3",
    "@heroicons/react": "v1",
    "clsx": "^1.2.1",
    "react-hot-toast": "^2.4.0"
  },
  "resolutions": {
    "webpack": "^5"
  }
}